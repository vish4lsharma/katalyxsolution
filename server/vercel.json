{
    "version": 2,
    "functions": {
        "index.js": {
            "maxDuration": 15
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/index.js"
        }
    ]
}